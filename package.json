    "next": "14.2.0",
    "next-auth": "^4.24.0",
    "nodemailer": "^7.0.12",
    "pdfjs-dist": "^5.4.530",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.0",
